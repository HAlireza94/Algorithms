   "source": [
    "def profit(prices):\n",
    "    prof = 0\n",
    "    for i in range(1,len(prices)):\n",
    "        if prices[i] > prices[i-1]:\n",
    "            prof += prices[i] - prices[i-1]\n",
    "    return prof\n",
    "# prices = [7,6,4,3,1]\n",
    "prices = [1,2,3,4,5]\n",
    "# prices = [7, 1, 5, 3, 6, 4]\n",
    "print(\"Maximum Profit:\", profit(prices))  "
   ]
